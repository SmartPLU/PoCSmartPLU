<script type="text/javascript" src="jquery-3.3.1.min.js"></script>

  <script type="text/javascript" src="jquery.imagemapster.js"></script>
  <script type="text/javascript">
    window.articlesPerZone = {} ;
    handleSelectedArea = function(area) {
	console.log("AREA SELECTED :" ) ;
    console.log(area) ;
    var txtContent = "<xml id=\"xmldata\">" ;
      window.articlesPerZone[area].forEach(function(article) {
      txtContent += article;
      }) ;
      txtContent += "</xml>" ;
    console.log(txtContent) ;
    $("#showArticles").get(0).innerHTML = txtContent ;

    } ;
    $(document).ready(function () {
	var opts = {
	    onConfigured : function(success) {
		// This dshows all the areas
		// $('area').mapster('set', true);
	    },
	    onClick : function(data) {
		$('area').mapster('set', false);
		handleSelectedArea(data.e.target.alt) ;
	    }
	}
	$('#image').mapster(opts) ;
    }) ;
      handleContent = function() {
      var content = $("#content").get(0).innerHTML ;
      var lines = content.split("\n") ;
      var articlesAndZones = [] ;
      lines.forEach(function(line) {
      if (line == "") return ;
      var parts = line.split(";", 2) ;
      articlesAndZones.push([parts[0], parts[1]]) ;
      });
    articlesAndZones.forEach(function(articleAndZone) {
    var zone = articleAndZone[0].replace(/^'/, "").replace(/'$/, "") ;
      if (!window.articlesPerZone[zone]) {
    window.articlesPerZone[zone] = [] ;
      }
    window.articlesPerZone[zone].push(articleAndZone[1].replace(/^'/, "").replace(/'$/, "").replace(/'+/g, "'")) ;
      }) ;
      console.log(window.articlesPerZone) ;
    }
  </script>

  <body onload="handleContent()">
  <body>
