    <style>
      #xmldata > article > par:nth-child(1) {
      display: block;
      font-size: 30;
      }
      
      #xmldata > article > par:nth-child(1) > line:nth-child(2) {
      display: block;
      font-size: 20;
      }

      #xmldata > article:nth-child(2) > block:nth-child(1) > text > par {
      display: block;
      font-size:  24;
      margin-left:  20;
      }
      
      #xmldata > article > block:nth-child(n+2) > text > par:nth-child(n+2) {
      display: block;
      font-size: 20;
      margin-left: 40;
      }
    </style>
