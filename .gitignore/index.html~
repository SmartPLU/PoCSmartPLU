la</w><w l="1118" r="1140" t="780" b="804" c="100" s="0"> </w><w l="1141" r="1339" t="771" b="813" c="100" s="0">composition</w><w l="1340" r="1365" t="772" b="804" c="100" s="0"> </w><w l="1366" r="1577" t="771" b="804" c="100" s="0">traditionnelle</w><w l="1578" r="1603" t="771" b="804" c="100" s="0"> </w><w l="1604" r="1699" t="771" b="804" c="100" s="0">locale</w><w l="1700" r="1723" t="771" b="804" c="100" s="0"> </w><w l="1724" r="1783" t="771" b="804" c="100" s="0">des</w><w l="1784" r="1808" t="771" b="804" c="100" s="0"> </w><w l="1809" r="1896" t="771" b="804" c="100" s="0">haies</w><w l="1897" r="1921" t="771" b="804" c="100" s="0"> </w><w l="1922" r="2102" t="771" b="813" c="100" s="0">bocagères.</w><w l="2103" r="2130" t="771" b="804" c="100" s="0"> </w><w l="2131" r="2189" t="771" b="804" c="100" s="0">Les</w><w l="2190" r="2214" t="771" b="804" c="100" s="0"> </w><w l="2215" r="2302" t="771" b="804" c="100" s="0">haies</w></formatting></line><line baseline="858" l="238" t="825" r="1293" b="867"><formatting lang="FrenchStandard" ff="Liberation Sans" fs="9"><w l="238" r="406" t="825" b="858" c="100" s="0">existantes</w><w l="407" r="417" t="825" b="858" c="100" s="0"> </w><w l="418" r="543" t="825" b="858" c="100" s="0">devront</w><w l="544" r="554" t="825" b="858" c="100" s="0"> </w><w l="555" r="617" t="825" b="858" c="100" s="0">être</w><w l="618" r="630" t="834" b="867" c="100" s="0"> </w><w l="631" r="815" t="825" b="867" c="100" s="0">préservées</w><w l="816" r="827" t="825" b="858" c="100" s="0"> </w><w l="828" r="908" t="825" b="858" c="100" s="0">dans</w><w l="909" r="920" t="825" b="858" c="100" s="0"> </w><w l="921" r="948" t="825" b="858" c="100" s="0">la</w><w l="949" r="960" t="834" b="858" c="100" s="0"> </w><w l="961" r="1083" t="834" b="858" c="100" s="0">mesure</w><w l="1084" r="1096" t="825" b="858" c="100" s="0"> </w><w l="1097" r="1135" t="825" b="858" c="100" s="0">du</w><w l="1136" r="1149" t="834" b="867" c="100" s="0"> </w><w l=